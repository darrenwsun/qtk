feature Cast the datatype of a column.

  before each
    .qtk.import.loadModule["db";`qtk];
    .qtk.import.loadModule["tbl";`qtk];
    .qtk.tbl.create[`PlainTable; ([] c1:1 2)];

  after each
    delete PlainTable from `.;

  should raise error "ColumnNotFoundError: [*]" if `column` doesn't exist.
    expect raise error "ColumnNotFoundError: [*]" if `column` doesn't exist.
      .qu.compare[
        @[.qtk.tbl.castColumn[`PlainTable; ; `float]; `c3; {x}];
        "ColumnNotFoundError: [c3]"]
